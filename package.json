{
  "name": "redis-navigator",
  "version": "0.0.1",
  "description": "Simple React Component to navigate through Redis kv storage",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "start": "parcel ./app/index.html",
    "extract-messages": "extract-messages -l=en,ru -o app/translations -d en --flat true --format yaml 'src/**/!(*.test).js'"
  },
  "jest": {
    "setupFiles": [
      "./test/jestsetup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/night-crawler/redis-navigator.git"
  },
  "keywords": [
    "redis",
    "navigator",
    "react"
  ],
  "author": "night-crawler",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/night-crawler/redis-navigator/issues"
  },
  "homepage": "https://github.com/night-crawler/redis-navigator#readme",
  "dependencies": {
    "codemirror": "^5.35.0",
    "collections": "^5.1.2",
    "es6-error": "^4.1.1",
    "eslint-plugin-flowtype": "^2.46.1",
    "file-type": "^7.6.0",
    "history": "^4.7.2",
    "immer": "^1.1.1",
    "intl": "^1.2.5",
    "is-base64": "^0.0.4",
    "js-cookie": "^2.2.0",
    "js-yaml": "^3.11.0",
    "jsoneditor": "^5.14.0",
    "prop-types": "^15.6.1",
    "react": "^16.3.0",
    "react-codemirror2": "^4.2.1",
    "react-dom": "^16.3.0",
    "react-helmet": "^5.2.0",
    "react-hotkeys": "^0.10.0",
    "react-intl": "^2.4.0",
    "react-json-view": "^1.16.1",
    "react-notification-system": "^0.2.17",
    "react-notification-system-redux": "^1.2.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-virtualized": "^9.18.5",
    "redux": "^3.7.2",
    "redux-api-middleware": "^2.2.0",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "semantic-ui-css": "^2.2.14",
    "semantic-ui-react": "^0.78.2",
    "styled-components": "^3.1.6",
    "url-search-params-polyfill": "^3.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.4.3",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-react-intl-auto": "^1.1.0",
    "babel-plugin-root-import": "^5.1.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "debug": "^3.1.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.1",
    "eslint": "^4.17.0",
    "eslint-plugin-jest": "^21.8.0",
    "eslint-plugin-react": "^7.6.1",
    "extract-react-intl-messages": "^0.8.2",
    "flow-bin": "^0.66.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.3.0",
    "jest-css-modules": "^1.1.0",
    "lodash": "^4.17.5",
    "parcel-bundler": "^1.5.1",
    "react-test-renderer": "^16.2.0",
    "redux-logger": "^3.0.6",
    "rewire": "^3.0.2"
  }
}
